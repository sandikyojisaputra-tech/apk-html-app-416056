<?xml version='1.0' encoding='utf-8'?>
<widget id="com.html.app" version="1.0.0" android-versionCode="1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HTML App</name>
    <description>App built with APK Studio</description>
    <author email="builder@apk-studio" href="https://github.com/sandikyojisaputra-tech">APK Studio</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="StatusBarBackgroundColor" value="#06060a" />
    <preference name="NavigationBarBackgroundColor" value="#06060a" />
    <preference name="android-minSdkVersion" value="24" />
    <preference name="android-targetSdkVersion" value="33" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    
    <platform name="android"><preference name="Orientation" value="default" /></platform>
</widget>